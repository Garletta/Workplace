<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Table.TBookEntity" table="T_BOOK">
        <id name="bookid" column="BOOKID" type="java.lang.String"></id>
        <property name="bookname" column="BOOKNAME" type="java.lang.String"></property>
        <property name="bookprice" column="BOOKPRICE" type="java.lang.Double"></property>
    </class>
</hibernate-mapping>